<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.screens.setings.ChangeUserNameFragment">

    <EditText
        android:id="@+id/settings_et_changeUser_name"
        style="@style/editRegistrText"
        android:inputType="text"
        android:hint="@string/settings_hint_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:autofillHints="@string/auto_fill_settings_et_change_user_name" />

</androidx.constraintlayout.widget.ConstraintLayout>